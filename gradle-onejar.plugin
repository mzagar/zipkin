buildscript {
	repositories {
		mavenLocal()
		jcenter()
		mavenCentral()
	}

	dependencies {
		classpath 'org.akhikhl.gradle-onejar:gradle-onejar:+'
	}
}

repositories {
	mavenLocal()
	jcenter()
	mavenCentral()
}

if (!project.plugins.findPlugin(org.akhikhl.gradle.onejar.OneJarPlugin))
	project.apply(plugin: org.akhikhl.gradle.onejar.OneJarPlugin)

